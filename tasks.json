{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: clang++ build active file",
			"command": "/usr/bin/clang++",
			"args": [
				"-std=c++17",
				"-g",
				"-Wall",
				"-arch",
				"arm64",
				"-O3",
				"-I${workspaceFolder}/project_Rambert/src",
				"${workspaceFolder}/project_SandBox/src/SandboxApp.cpp",
				"-L${workspaceFolder}/project_Rambert/src/Rambert",
				"-lapplib",
				"-o",
				"/Users/Dev/workspace_Rambert/bin/Release-x64/Sandbox/SandboxApp",
				"-framework",
				"OpenGl"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/clang++"
		}
	]
}