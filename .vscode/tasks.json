{
	"version": "2.0.0",
	"tasks": [
	 {
	  "type": "cppbuild",
	  "label": "C/C++: clang++ build active file, Ted",
	  "command": "/usr/bin/clang++",
	  "args": [
	   "-std=c++17",
	   "-fdiagnostics-color=always",
	   "-Wall",
	   "-g",
	   "-I${workspaceFolder}/project_SandBox/dependencies/include",
	   "-I${workspaceFolder}/project_Rambert/src",
	   "-I${workspaceFolder}/project_SandBox/vendor",
	   "-L${workspaceFolder}/project_Rambert/src",
	   "${workspaceFolder}/project_SandBox/src/*.cpp",
	   "${workspaceFolder}/project_Rambert/src/Rambert/Application.cpp",
	   "${workspaceFolder}/project_Rambert/src/libmylib.dylib",
	   "-o",
	   "${workspaceFolder}/bin/Release-x64/Sandbox/app",
	   "-framework",
	   "OpenGL"
	  ],
	  "options": {
	   "cwd": "${fileDirname}"
	  },
	  "problemMatcher": ["$gcc"],
	  "group": {
	   "kind": "build",
	   "isDefault": true
	  },
	  "detail": "compiler: /usr/bin/clang++"
	 }
	]
   }